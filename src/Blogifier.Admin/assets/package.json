  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "bootstrap": "~5.1.0",
    "glob": "^10.2.6",
    "rollup": "^3.23.1",
    "rtlcss": "^3.3.0",
    "sass": "^1.32.12"
  },
  "dependencies": {
    "autosize": "^6.0.1",
    "easymde": "^2.18.0",
    "highlight.js": "^11.8.0"
  }
}
